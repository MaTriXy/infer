  "scheduler": "callgraph",
  "pulse-only": true,
  "pulse-specialization-partial": true,
  "pulse-taint-short-traces": true,
  "pulse-taint-sources": [
    { "procedure": "level0::taint_source" },
    { "procedure": "level1::taint_source" }
  ],
  "pulse-taint-sinks": [
    { "procedure": "level0::taint_sink",
      "taint_target": ["ArgumentPositions", [1]]
    },            
    { "procedure": "level1::taint_sink",
      "taint_target": ["ArgumentPositions", [1]]
    }
  ]
}
